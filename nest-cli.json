{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/event-microservice/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/event-microservice/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/event-microservice",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "event-microservice": {
      "type": "application",
      "root": "apps/event-microservice",
      "entryFile": "main",
      "sourceRoot": "apps/event-microservice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/event-microservice/tsconfig.app.json"
      }
    },
    "events-service": {
      "type": "application",
      "root": "apps/events-service",
      "entryFile": "main",
      "sourceRoot": "apps/events-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/events-service/tsconfig.app.json"
      }
    },
    "friends": {
      "type": "application",
      "root": "apps/friends",
      "entryFile": "main",
      "sourceRoot": "apps/friends/src",
      "compilerOptions": {
        "tsConfigPath": "apps/friends/tsconfig.app.json"
      }
    },
    "users-service": {
      "type": "application",
      "root": "apps/users-service",
      "entryFile": "main",
      "sourceRoot": "apps/users-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users-service/tsconfig.app.json"
      }
    }
  }
}